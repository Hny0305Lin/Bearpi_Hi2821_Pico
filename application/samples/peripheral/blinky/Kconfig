config BLINKY_PIN
    int
    prompt "Choose BLINKY_PIN pin."
    depends on SAMPLE_SUPPORT_BLINKY
    default 31

config BLINKY_DURATION_MS
    int
    prompt "Duration of blinky in MS."
    default 500